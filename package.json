{
  "name": "team2-library-web-app",
  "version": "1.0.0",
  "description": "A basic Express TypeScript app using ES modules",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
