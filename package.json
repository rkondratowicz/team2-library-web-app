    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.8.0",
    "@types/sqlite3": "^3.1.11",
    "typescript": "^5.2.0"
  }
